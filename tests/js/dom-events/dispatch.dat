#data
    var e1 = document.createElement("div1");
    var e2 = document.createElement("div2");    
    var e3 = document.createElement("div3");
    var e4 = document.createElement("div4");
    
    e1.appendChild(e2);
    e2.appendChild(e3);
    e3.appendChild(e4);

    var callback = function (event) {
        console.log(this.tagName);
        console.log(event.type);            
        console.log(event.target.tagName);        
        console.log(event.currentTarget.tagName);
        console.log(event.eventPhase);    
        console.log(event.bubbles);        
        console.log(event.cancelable);      
        console.log(event.defaultPrevented);
        //console.log(event.composed);       
        //console.log(event.isTrusted);       
    }; 
    
    e1.addEventListener("click", callback);
    e2.addEventListener("click", callback);
    e3.addEventListener("click", callback);
    e4.addEventListener("click", callback);

    var v = new Event("click");  
    e4.dispatchEvent(v);
#document
