@===============================================================================
@           Document.insertAdjacentHTML
@===============================================================================

#data
<html id="id1">
<head></head>
<body>
    <p id="id2"></p>
    <script>
        var e1 = document.getElementById("id1");
        var e2 = document.getElementById("id2");
           
        try {
            e1.insertAdjacentHTML("afterend", "<div></div><p></p>");
        } catch (e) {
            console.log("e: " + e.name);
        }

        try {
            e2.insertAdjacentHTML("before", "<div></div><p></p>");
        } catch (e) {
            console.log("e: " + e.name);
        }
    </script>
</body>
</html>
#document
e: NoModificationAllowedError
e: SyntaxError
